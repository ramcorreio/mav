<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="classpath:mav-service-context.xml" />
	
	<bean id="trapHandler" class="com.stefanini.mav.tcp.MensagemIoHandler" />
	<bean id="inputCodec" class="com.stefanini.mav.tcp.MensagemCodecFactory" />
	<bean id="inputPort" class="java.lang.Integer">
		<constructor-arg value="8889" />
	</bean>
	<import resource="classpath:mav-deamon-template.xml" />
	<import resource="classpath:mav-parceira-loader-context.xml" />
</beans>