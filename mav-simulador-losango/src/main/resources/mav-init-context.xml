<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="trapHandler" class="com.stefanini.mav.simulador.Losango" />
	<bean id="inputCodec" class="org.apache.mina.filter.codec.textline.TextLineCodecFactory" />
	<bean id="inputPort" class="java.lang.Integer">
		<constructor-arg value="9089" />
	</bean>
	<import resource="classpath:mav-deamon-template.xml" />	

</beans>